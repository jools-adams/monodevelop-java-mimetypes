<Addin namespace   = "MonoDevelop"
       id          = "Java.Mimetypes"  
       name        = "Java Mimetypes"  
       author      = "Julian Adams"
       copyright   = "MIT/X11"
       url         = "http://www.monodevelop.com"
       description = "Adds x-java mimetype to .java files"
       category    = "Source Editor Extensions"
       version     = "1.0">
       
    <Dependencies>  
        <Addin id="Ide" version="4.0"/>  
<!--        <Addin id="SourceEditor2" version="4.0"/>  -->
    </Dependencies> 
    
    <Extension path = "/MonoDevelop/Core/MimeTypes">  
      <MimeType id="text/x-java" _description="Java Source Files" isText="true"> 
        <File pattern="*.java" />  
      </MimeType>  
    </Extension>  
      
</Addin>
